{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "System": "Warning",
      "Microsoft": "Warning"
    },
    "File": {
      "FolderPath": ".\\",
      "FilePath": "log_{date}.log"
    },
    "Console": {
      "FormatterName": "CustomConsoleFormatter",
      "FormatterOptions": {
        "IncludeScopes": true,
        "EnableColors": true
      }
    }
  }
}